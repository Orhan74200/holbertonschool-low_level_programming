	srand(time(0));
	n = rand() - RAND_MAX / 2;

	last_digit = n % 10;

	printf("Last digit of %d is %d ", n, last_digit);

	if(last_digit > 5)
	  {
	    printf("and is greater than 5\n");
	  } else if
	 
	return (0);
}
